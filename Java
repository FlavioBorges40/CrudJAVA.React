SQL

CREATE TABLE Notifications(
	id varchar(255),
	name varchar(255),
	msg varchar(255),
	primary key(id)
)

SELECT * FROM Notifications

INSERT INTO Notifications(name,msg) VALUES ('Gatts','Teste')

DROP TABLE Notifications